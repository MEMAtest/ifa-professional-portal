{
  "name": "plannetic-platform",
  "version": "0.1.1",
  "description": "Strategic Planning in Motion - Professional Financial Advisory Platform",
  "private": true,
  "keywords": [
    "financial-planning",
    "advisory-platform",
    "cash-flow-modeling",
    "risk-assessment",
    "strategic-planning"
  ],
  "author": "Plannetic Team",
  "homepage": "https://plannetic.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/plannetic-platform.git"
  },
  "scripts": {
    "dev": "next dev",
    "dev:clean": "node -e \"const fs=require('fs'); for (const name of fs.readdirSync('.')) { if (name==='.next' || name.startsWith('.next-dev-')) fs.rmSync(name,{recursive:true,force:true}); }\" && next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:legacy": "node --test --loader ./scripts/ts-loader.mjs tests/*.test.ts",
    "lint": "next lint",
    "e2e": "playwright test",
    "e2e:mobile": "playwright test --project=mobile-chrome",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report test-results/playwright-report",
    "e2e:chromium": "playwright test --project=chromium",
    "e2e:firefox": "playwright test --project=firefox",
    "e2e:webkit": "playwright test --project=webkit",
    "e2e:all": "playwright test --project=chromium --project=firefox --project=webkit",
    "e2e:update-snapshots": "playwright test --update-snapshots",
    "e2e:install": "playwright install --with-deps",
    "qa:suitability": "playwright test --project=qa-suitability",
    "qa:brutal": "playwright test --project=qa-brutal",
    "qa:conditional": "playwright test e2e/qa-agent/runners/conditionalLogic.spec.ts --project=qa-brutal",
    "qa:stress": "playwright test e2e/qa-agent/runners/apiStress.spec.ts --project=qa-brutal",
    "qa:state": "playwright test e2e/qa-agent/runners/stateManagement.spec.ts --project=qa-brutal",
    "qa:destruction": "playwright test e2e/qa-agent/runners/formDestruction.spec.ts --project=qa-brutal",
    "qa:report": "playwright show-report test-results/html-report",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report test-results/playwright-report",
    "db:migrate": "supabase migration up",
    "db:reset": "supabase db reset",
    "gen:types": "npx supabase gen types typescript --project-id maandodhonjolrmcxivo > src/types/database.types.ts",
    "type-check": "tsc --noEmit",
    "db:backfill-firm-id": "node scripts/backfill-firm-id.mjs",
    "db:backfill-firm-id:dry": "node scripts/backfill-firm-id.mjs --dry-run",
    "demo:seed": "node scripts/seed-demo.mjs",
    "demo:reset": "node scripts/reset-demo.mjs",
    "stripe:bootstrap": "node scripts/stripe/bootstrap-pricing.mjs"
  },
  "dependencies": {
    "@aws-sdk/client-sesv2": "^3.980.0",
    "@hookform/resolvers": "^3.3.4",
    "@kenjiuno/msgreader": "^1.28.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.0.2",
